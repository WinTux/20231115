@model PasosIniciales.Models.Producto3
<h3>Modificar producto</h3>
<form asp-controller="ef" asp-action="modificar" method="post">
    <table class="table">
        <tr>
            <td>ID</td>
            <td><input type="text" asp-for="Id" /></td>
        </tr>
        <tr>
            <td>Nombre</td>
            <td><input type="text" asp-for="Nombre" /></td>

        </tr>
        <tr>
            <td>Precio</td>
            <td><input type="text" asp-for="Precio" /></td>

        </tr>
        <tr>
            <td>Foto</td>
            <td><input type="text" asp-for="Foto" /></td>
        </tr>
        <tr>
            <td>
                <input type="submit" value="MODIFICAR" />
            </td>
        </tr>
    </table>
</form>